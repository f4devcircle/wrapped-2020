	<meta property="og:description" content="Dapatkan ringkasan #jkt48wrapped<%= process.env.YEAR %> kamu sekarang!">
	<meta property="og:image" content="https://<%= process.env.YEAR %>.ngidol.club/share/<%= data.slug %>.png">

	<!-- Twitter Meta Tags -->
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:site:" content="f4devcircle">
	<meta property="twitter:domain" content="<%= process.env.YEAR %>.ngidol.club">
	<meta property="twitter:url" content="https://<%= process.env.YEAR %>.ngidol.club/share/<%= data.slug %>.html">
	<meta name="twitter:title" content="#jkt48wrapped<%= process.env.YEAR %> untuk <%= data.name %>">
	<meta name="twitter:description" content="Dapatkan ringkasan #jkt48wrapped<%= process.env.YEAR %> kamu sekarang!">
	<meta name="twitter:image" content="https://<%= process.env.YEAR %>.ngidol.club/share/<%= data.slug %>.png">

	<meta name="viewport" content="width=device-width, initial-scale=1">

	<link rel="icon" type="image/png" href="https://storage.googleapis.com/ngidol.club/F4DCLogoTrans2.png"/>
	<link rel="stylesheet" type="text/css" href="../vendor/bootstrap/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="../fonts/font-awesome-4.7.0/css/font-awesome.min.css">
	<link rel="stylesheet" type="text/css" href="../fonts/iconic/css/material-design-iconic-font.min.css">
	<link rel="stylesheet" type="text/css" href="../vendor/animate/animate.css">
	<link rel="stylesheet" type="text/css" href="../vendor/css-hamburgers/hamburgers.min.css">
	<link rel="stylesheet" type="text/css" href="../vendor/animsition/css/animsition.min.css">
	<link rel="stylesheet" type="text/css" href="../vendor/select2/select2.min.css">
	<link rel="stylesheet" type="text/css" href="../vendor/daterangepicker/daterangepicker.css">
	<link rel="stylesheet" type="text/css" href="../css/util.css">
	<link rel="stylesheet" type="text/css" href="../css/main.css">

	<!-- Global site tag (gtag.js) - Google Analytics -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=G-1EM4793VWK"></script>
